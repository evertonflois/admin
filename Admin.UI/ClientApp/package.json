{
  "name": "Admin.ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "node aspnetcore-https",
    "start": "run-script-os",
    "start:windows": "ng serve --port 44421 --ssl --ssl-cert \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem\" --ssl-key \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.key\"",
    "start:default": "ng serve --port 44421 --ssl --ssl-cert \"$HOME/.aspnet/https/${npm_package_name}.pem\" --ssl-key \"$HOME/.aspnet/https/${npm_package_name}.key\"",
    "build": "ng build",
    "build:ssr": "ng run Admin.UI:server:dev",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build-layout": "sass src/assets/saas/layout-noir.scss src/assets/layout/css/layout-Admin.css",
    "build-theme": "sass src/assets/saas/theme-noir.scss src/assets/theme/theme-Admin.css"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^17.0.5",
    "@angular/animations": "^17.0.5",
    "@angular/cdk": "^17.0.2",
    "@angular/cli": "^17.0.5",
    "@angular/common": "^17.0.5",
    "@angular/compiler": "^17.0.5",
    "@angular/compiler-cli": "^17.0.5",
    "@angular/core": "^17.0.5",
    "@angular/forms": "^17.0.5",
    "@angular/platform-browser": "^17.0.5",
    "@angular/platform-browser-dynamic": "^17.0.5",
    "@angular/platform-server": "^17.0.5",
    "@angular/router": "^17.0.5",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "chart.js": "3.3.2",
    "crypto-js": "^4.2.0",
    "css-loader": "^6.8.1",
    "file-saver": "^2.0.5",
    "jquery": "^3.7.1",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.8.1",
    "ngx-spinner": "^16.0.2",
    "oidc-client": "^1.11.5",
    "popper.js": "^1.16.0",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
